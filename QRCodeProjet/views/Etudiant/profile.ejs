<!doctype html>
<html>
<head>
	<title>Profil</title>
    <% include ../layout%>
</head>
<body>
<div class="container">

	<div class="page-header text-center">
		<h1>Profil</h1>
		<a href="/logout" class="btn btn-default btn-sm">Déconnexion</a>
	</div>

	<div class="row">
		<div class="col-sm-6">
			<div class="well">
				<h3><span class="fa fa-user"></span> Local</h3>
				<p>
					<strong>Username</strong>: <%= user.username %><br>
					<strong>Nom</strong>: <%= user.nomU %><br>
					<strong>Prénom</strong>: <%= user.prenomU %>
				</p>
			</div>
		</div>
	</div>

</div>
</body>
</html>
