<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Node Admin</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <style type="text/css">
        .bs-example{
            margin: 20px;
        }
        body 		{
            padding-top:80px;
        }

    </style>
</head>
<body>

<div class="container" id="MainMenu">
    <div class="list-group panel">
        <a href="#etudiant" class="list-group-item " data-toggle="collapse" data-parent="#MainMenu"><strong>Etudiants</strong></a>
        <div class="collapse" id="etudiant">
            <% for(var i=0; i<=data.length-1; i++) {%>
            <a href="/profile" class="list-group-item"><%=JSON.stringify(data[i].id)%> - <%=JSON.stringify(data[i].nomU)%> - <%=JSON.stringify(data[i].prenomU)%> - <%=JSON.stringify(data[i].promotionU)%></a>
            <% } %>
        </div>
        <a href="#enseignant" class="list-group-item" data-toggle="collapse" data-parent="#MainMenu"><strong>Enseignants</strong></a>
        <div class="collapse" id="enseignant">
            <a href="/profile" class="list-group-item">enseignants 1</a>
            <a href="/profile" class="list-group-item">enseignants 2</a>
            <a href="/profile" class="list-group-item">enseignants 3</a>
            <a href="/profile" class="list-group-item">enseignants 4</a>
        </div>
        <a href="#administration" class="list-group-item" data-toggle="collapse" data-parent="#MainMenu"><strong>Administration</strong></a>
        <div class="collapse" id="administration">
            <a href="/profile" class="list-group-item">Subitem 1</a>
            <a href="/profile" class="list-group-item">Subitem 2</a>
            <a href="/profile" class="list-group-item">Subitem 3</a>
        </div>
    </div>


</div>
</body>
</html>
